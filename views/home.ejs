<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>NLMX - Data</title>
    <link rel="stylesheet" href="view-source:https://bootswatch.com/cosmo/">
    <link rel="stylesheet" href="https://bootswatch.com/4/cosmo/bootstrap.css">
</head>
<body>
<div class="fixed-top">
        <nav class="navbar navbar-expand-lg navbar-dark bg-primary">
                <a class="navbar-brand" href="#">Bienvenido a NLMX Data</a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarColor01" aria-controls="navbarColor01" aria-expanded="false" aria-label="Toggle navigation">
                  <span class="navbar-toggler-icon"></span>
                </button>
              </nav>
</div>

<div class="row" style="margin-top:125px;margin-left:40%; margin-right:10%; width: fit-content;">
    <div class="lg-6">
            <div class="card border border-primary mb-3" style="max-width: 20rem;">
                    <div class="card-header">Upload Excel File</div>
                        <div class="card-body">
                          <form action="/home" method="POST" enctype="multipart/form-data">
                         <div class="form-group">
                           <label for="excel">BrowseFile</label><input type="file" class="form-control" name="excel" required><br><br>
                           <input type="submit" value="submit">
                         </div>
                          </form>
                        </div>
                    </div>
                </div>
</div><br><br>

<%if(result.length>0){%>
  <table class="table table-hover" border="1">
    <thead>
    <tr>
      <th>BranchF553007MMCU</th>
      <th>MonthDescriptionEffectiveFromEFFF</th>
      <th>BusinessUnitF553007MCU</th>
      <th>BranchF553007MMCU</th>
      <th>EffectiveFromF553007EFFFF</th>
      <th>WeekNumberEffectiveFromEFFF</th>
      <th>PlannedReleasedRFGA</th>
      <th>FirmWO</th>
      <th>PlannedWO</th>
      <th>DailyCapacityRFGA</th>
      <th>WeeklyCapacityRFGA</th>
      <th>MothlyCapactyRFGA</th>
      <th>RequestDateF553312DRQJ</th>
      <th>RateHourRFGA</th>
      <th>PrimaryUOMHour</th>
      <th>ShortItemNoF553312ITM</th>
      <th>SecondItemNumberLITM</th>
      <th>WorkOrderQuantity</th>
      <th>QuantityOrdered</th>
      <th>WorkOrderNo</th>
      <th>WOStatus</th>
      <th>TypeofRouting</th>
      <th>WOStartDate</th>
    </tr>
  </thead>
  <tbody>
    <%for(var x=0;x < result.length;x++){%>
      <tr>
        <td><%= result[x].BranchF553007MMCU %></td>
        <td><%= result[x].MonthDescriptionEffectiveFromEFFF %></td>
        <td><%= result[x].BusinessUnitF553007MCU %></td>
        <td><%= result[x].BranchF553007MMCU %></td>
        <!--<td><%= result[x].EffectiveFromF553007EFFFF %></td>-->
        <td><%= result[x].WeekNumberEffectiveFromEFFF %></td>
        <td><%= result[x].PlannedReleasedRFGA %></td>
        <td><%= result[x].FirmWO %></td>
        <td><%= result[x].PlannedWO %></td>
        <td><%= result[x].DailyCapacityRFGA %></td>
        <td><%= result[x].WeeklyCapacityRFGA %></td>
        <td><%= result[x].MothlyCapactyRFGA %></td>
        <td><%= result[x].RequestDateF553312DRQJ %></td>
        <td><%= result[x].RateHourRFGA %></td>
        <td><%= result[x].PrimaryUOMHour %></td>
        <td><%= result[x].ShortItemNoF553312ITM %></td>
        <td><%= result[x].SecondItemNumberLITM %></td>
        <td><%= result[x].WorkOrderQuantity %></td>
        <td><%= result[x].QuantityOrdered %></td>
        <td><%= result[x].WorkOrderNo %></td>
        <td><%= result[x].WOStatus %></td>
        <td><%= result[x].TypeofRouting %></td>
        <td><%= result[x].WOStartDate %></td>
      </tr>
      <%}%>
  </tbody>
  </table>
<%}%>


<script src="https://code.jquery.com/jquery-3.4.1.slim.min.js" integrity="sha384-J6qa4849blE2+poT4WnyKhv5vZF5SrPo0iEjwBvKU7imGFAV0wwj1yYfoRSJoZ+n" crossorigin="anonymous"></script>
<script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
<script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js" integrity="sha384-wfSDF2E50Y2D1uUdj0O3uMBJnjuUD4Ih7YwaYd1iqfktj0Uod8GCExl3Og8ifwB6" crossorigin="anonymous"></script>   
</body>
</html>